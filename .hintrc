{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "css-prefix-order": "off",
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "button-type": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not firefox <= 128",
    "not safari <= 17.5",
    "not and_chr <= 126",
    "not chrome <= 126",
    "not edge <= 126",
    "not ios_saf <= 17.5",
    "not opera <= 111",
    "not samsung <= 25"
  ]
}